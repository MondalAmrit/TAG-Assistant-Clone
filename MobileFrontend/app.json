{
  "expo": {
    "name": "NOVA",
    "slug": "MobileFrontend",
    "version": "1.0.0",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/models/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.NOVA.Assistant",
      "permissions": [
        "BLUETOOTH",
        "BLUETOOTH_CONNECT",
        "BLUETOOTH_SCAN",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png",
      "resolver": {
        "unstable_enablePackageExports": true
      }
    },
    "plugins": [
      "expo-router",
      "onnxruntime-react-native",
      [
        "expo-asset",
        {
          "assets": [
            "./assets/models/GPT_MODEL.with_runtime_opt.ort",
            "./assets/images/logo.png",
            "./assets/nova_tokenizer/tokenizer.json",
            "./assets/nova_tokenizer/tokenizer_config.json"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7ae8bf65-37b3-4b57-9ff0-3bf2651d16cf"
      },
      "resolver": {
        "unstable_enablePackageExports": true
      }
    }
  }
}
